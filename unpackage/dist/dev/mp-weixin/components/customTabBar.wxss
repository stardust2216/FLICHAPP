/* 水平间距 */
@font-face {
  font-family: "MyCustomFont";
  src: url("../static/fonts/XiangJiaoDaJiangJunLingGanTi-2.ttf") format("opentype");
}
.navbar {
  z-index: 10;
}
.navbar .titleBar {
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.navbar .titleBar .left {
  margin-left: 42rpx;
  float: left;
}
.navbar .titleBar .left text {
  font-family: "MyCustomFont";
  color: #696a60;
  font-size: 40rpx;
  font-weight: bold;
}
.navbar .titleBar .right {
  display: flex;
  align-items: center;
  margin-right: 210rpx;
}
.navbar .titleBar .right image {
  width: 45rpx;
  height: 45rpx;
  margin-right: 25rpx;
}
.navbar .titleBar .right text {
  font-size: 25rpx;
  font-weight: bold;
}